








function W = reshapeWeights(NN, layerFrom)
     if (strcmp( class (NN), "NeuralNetwork"))
           layerTo = layerFrom+1;
           start = 0;
           for i = 2:layerFrom
               start = NN.shape(i-1) * NN.shape(i)
               
     else 
         usage = "Not using a Neural Network"
     end
end
